{
  "devCommand": "npm run dev",
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/index.ts"
    }
  ],
  "crons": [
    {
      "path": "/src/runScraping.ts",
      "schedule": "*/5 * * * *"
    }
  ]
}
